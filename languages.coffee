@REPLIT.Languages =
  QBasic:
    name: 'QBasic'
    tagline: 'Structured programming for beginners.'
    shortcut: 'Q'
    category: 'Classic'
    examples:
      editor: '/langs/qbasic/examples-editor.txt'
      console: '/langs/qbasic/examples-console.txt'
    help_link: 'http://westcompsci.pima.edu/cis100/'
  Forth: 
    name: 'Forth'
    tagline: 'An interactive stack-oriented language.'
    shortcut: 'r'
    category: 'Classic'
    examples:
      editor: '/langs/forth/examples-editor.txt'
      console: '/langs/forth/examples-console.txt'      
  Smalltalk:
    name: 'Smalltalk'
    tagline: 'The grand-daddy of OOP.'
    shortcut: 't'
    category: 'Classic'
    examples:
      editor: '/langs/smalltalk/examples-editor.txt'
      console: '/langs/smalltalk/examples-console.txt'
  LOLCODE:
    name: 'LOLCODE'
    tagline: 'The BASIC language of lolcats.'
    shortcut: 'O'
    category: 'Esoteric'
    examples:
      editor: '/langs/lolcode/examples-editor.txt'
      console: '/langs/lolcode/examples-console.txt'
    help_link: 'http://lolcode.com/specs/1.2'
  Brainfuck:
    name: 'Brainf*ck'
    tagline: 'A pure Turing machine controller.'
    shortcut: 'f'
    category: 'Esoteric'
    examples:
      editor: '/langs/brainfuck/examples-editor.txt'
      console: '/langs/brainfuck/examples-console.txt'
    help_link: 'http://en.wikipedia.org/wiki/Brainfuck'
  Emoticon:
    name: 'Emoticon'
    tagline: 'Programming with an extra dose of smile.'
    shortcut: 'E'
    category: 'Esoteric'
    examples:
      editor: '/langs/emoticon/examples-editor.txt'
      console: '/langs/emoticon/examples-console.txt'
    help_link: 'http://www.teuton.org/~stranger/code/emoticon/manual.php'
  Bloop:
    name: 'Bloop'
    tagline: 'Nothing but bounded loops'
    shortcut: 'B'
    category: 'Esoteric'
    examples:
      editor: '/langs/bloop/examples-editor.txt'
      console: '/langs/bloop/examples-console.txt'
    example_file: '/langs/bloop/examples.txt'
  Unlambda:
    name: 'Unlambda'
    tagline: 'Functional purity given form.'
    shortcut: 'U'
    category: 'Esoteric'
    example_file: '/langs/unlambda/examples.txt'
    examples:
      editor: '/langs/unlambda/examples-editor.txt'
      console: '/langs/unlambda/examples-console.txt'
    help_link: 'http://en.wikipedia.org/wiki/Unlambda'
  JavaScript:
    name: 'JavaScript'
    tagline: 'The de facto language of the Web.'
    shortcut: 'J'
    category: 'Web'
    example_file: '/langs/javascript/examples.txt'
    help_link: 'http://en.wikipedia.org/wiki/Javascript'
    examples:
      editor: '/langs/javascript/examples-editor.txt'
      console: '/langs/javascript/examples-console.txt'
    ace_mode:
      script: '/lib/ace/mode-javascript.js'
      module: 'ace/mode/javascript'
  Traceur:
    name: 'Javascript.next'
    tagline: 'The JavaScript of tomorrow.'
    shortcut: 'n'
    category: 'Web'
    examples:
      editor: '/langs/traceur/examples-editor.txt'
      console: '/langs/traceur/examples-console.txt'
  CoffeeScript:
    name: 'CoffeeScript'
    tagline: 'Unfancy JavaScript.'
    shortcut: 'C'
    category: 'Web'
    examples:
      editor: '/langs/coffee-script/examples-editor.txt'
      console: '/langs/coffee-script/examples-console.txt'
    help_link: 'http://jashkenas.github.com/coffee-script/'
    ace_mode:
      script: '/lib/ace/mode-coffee.js'
      module: 'ace/mode/coffee'
  Kaffeine:
    name: 'Kaffeine'
    tagline: 'Extended JavaScript for pros.'
    shortcut: 'K'
    category: 'Web'
    examples:
      editor: '/langs/kaffeine/examples-editor.txt'
      console: '/langs/kaffeine/examples-console.txt'
    help_link: 'http://weepy.github.com/kaffeine/'
  Move:
    name: 'Move'
    tagline: 'The easy way to program the web.'
    shortcut: 'M'
    category: 'Web'
    examples:
      editor: '/langs/move/examples-editor.txt'
      console: '/langs/move/examples-console.txt'
    example_file: '/langs/move/examples.txt'
    help_link: 'http://movelang.org/'
  Scheme:
    name: 'Scheme'
    tagline: 'An elegant dynamic dialect of Lisp.'
    shortcut: 'S'
    category: 'Practical'
    examples:
      editor: '/langs/scheme/examples-editor.txt'
      console: '/langs/scheme/examples-console.txt'
    example_file: '/langs/scheme/examples.txt'
    help_link: 'http://en.wikipedia.org/wiki/Scheme_(programming_language)'
  Lua:
    name: 'Lua'
    tagline: 'A lightweight multi-paradigm scripting language.'
    shortcut: 'L'
    category: 'Practical'
    example_file: '/langs/lua/examples.txt'
    help_link: 'http://en.wikipedia.org/wiki/Lua_(programming_language)'
    examples:
      editor: '/langs/lua/examples-editor.txt'
      console: '/langs/lua/examples-console.txt'
    ace_mode:
      script: '/lib/ace/mode-lua.js'
      module: 'ace/mode/lua'
  Python:
    name: 'Python'
    tagline: 'A dynamic language emphasizing readabillity.'
    shortcut: 'P'
    category: 'Practical'
    examples:
      editor: '/langs/python/examples-editor.txt'
      console: '/langs/python/examples-console.txt'
    help_link: 'http://en.wikipedia.org/wiki/Python_(programming_language)'
    ace_mode:
      script: '/lib/ace/mode-python.js'
      module: 'ace/mode/python'
